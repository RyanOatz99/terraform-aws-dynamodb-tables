provider "aws" {
  # Use the profile specified in config/config-dev.yml
  profile = "{{Profile}}"
  region  = "eu-west-1"
}

module "table" {
  source = "../../../../"
  table_info = [
    {
      name = "{{ProjectAcronym}}Table1"
    },
    {
      name = "{{ProjectAcronym}}Table2"
      read_capacity = 2
      write_capacity = 2
    },
  ]
}
